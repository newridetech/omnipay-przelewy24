  "keywords": [
    "przelewy24",
    "gateway",
    "merchant",
    "omnipay",
    "pay",
    "payment",
    "transfer"
  ],
  "homepage": "https://github.com/newridetech/omnipay-przelewy24",
  "license": "MIT",
  "authors": [
    {
      "name": "TicketSwap",
      "email": "info@ticketswap.com"
    }
